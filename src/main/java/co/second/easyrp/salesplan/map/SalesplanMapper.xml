<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.second.easyrp.salesplan.map.SalesplanMapper">
	<select id="SalesplanSelectList">
		select 
			cod,
			client_cod,
			product_cod,
			plan_date,
			basicpln_qty,
			modpln_qty,
			state_cod
		from salesplan
	</select>
	<select id="SalesplanSelect"></select>
	<insert id="SalesplanInsert"></insert>
	<update id="SalesplanUpdate"></update>
	<delete id="SalesplanDelete"></delete>
</mapper>